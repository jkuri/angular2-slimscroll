{
  "name": "ngx-slimscroll",
  "version": "3.4.1",
  "description": "Angular2+ Customizable Scrollbar",
  "main": "bundles/ngx-slimscroll.umd.js",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jkuri/ngx-slimscroll.git"
  },
  "scripts": {
    "start": "webpack-dev-server --env.client --env.serve --progress",
    "start:aot": "webpack-dev-server --env.client --env.serve --env.aot --progress",
    "build": "webpack --env.client --env.aot --progress --hide-modules",
    "build:prod": "webpack --env.client --env.aot -p --progress --hide-modules",
    "prebuild": "npm run clean",
    "prebuild:prod": "npm run clean",
    "clean": "rimraf dist ./**/*.ngsummary.*",
    "prepublishOnly": "ngc -p tsconfig.json && webpack --config webpack.bundle.js -p",
    "lint": "tslint ./src/**/*.ts",
    "test": "karma start"
  },
  "keywords": [
    "ng2",
    "angular",
    "angular2",
    "scroll",
    "scrollbar",
    "slimscroll"
  ],
  "author": "Jan Kuri <jkuri88@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@angular/common": "^4.3.5",
    "@angular/compiler": "^4.3.5",
    "@angular/compiler-cli": "^4.3.5",
    "@angular/core": "^4.3.5",
    "@angular/platform-browser": "^4.3.5",
    "@angular/platform-browser-dynamic": "^4.3.5",
    "@angular/router": "^4.3.5",
    "@ngtools/webpack": "^1.6.1",
    "@types/jasmine": "^2.5.53",
    "@types/node": "^8.0.24",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.2.3",
    "bulma": "^0.5.1",
    "codelyzer": "^3.1.2",
    "compression-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.5.0",
    "css-loader": "^0.28.5",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "jasmine-core": "^2.7.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.4",
    "node-sass": "^4.5.3",
    "portfinder": "^1.0.13",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^2.6.1",
    "rxjs": "^5.4.3",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "tslint": "^5.6.0",
    "typescript": "^2.4.2",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0",
    "zone.js": "^0.8.16"
  }
}
